{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "amd": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "globals": {
    
  },
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "no-console": "off",
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120,
        "semi": false,
        "singleQuote": true,
        "endOfLine": "lf",
        "proseWrap": "never"
      }
    ]
  }
}